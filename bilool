name : Run TelegramBotn 24/7
on:
push:
branches: [main]
jobs:
run-bot:
runs-on: ubuntu-latest
steps:
- uses:actions/checkout@v3
- name: Set up Python
user: actions/setup-python@v4
with:
  python-version:  '3.10'
  name:Install dependecies
  run: pip install - r requircents txt 
  name:Run bot 
  run: python app.py
  
